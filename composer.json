{
    "name": "mehrdadmehralian/php-sep-refund",
    "description": "PHP SEP Refund Service JMS metadata",
    "type": "library",
    "authors": [
        {
            "name": "Mehrdad Mehralian",
            "email": "mehralian_mehrdad@yahoo.com"
        }
    ],
    "minimum-stability": "stable",
    "require": {
      "php": "^5.5|^7.0",
      "goetas-webservices/xsd2php-runtime":"^0.2.2",
      "goetas-webservices/soap-client": "^0.1.3",
      "php-http/guzzle6-adapter": "^1.0",
      "php-http/message": "^1.0",
      "guzzlehttp/psr7": "^1.3",
      "symfony/var-dumper": "*",
        "mehrdadmehralian/php-sep-refund": "@dev"
    },
    "require-dev": {
      "goetas-webservices/wsdl2php": "^0.2",
      "goetas-webservices/xsd2php":"^0.2"
    },
    "autoload": {
      "psr-4": {
        "Calculator\\": "src/Calculator",
        "IsrvRefund\\": "src/IsrvRefund",
        "Serialization\\": "src/Serialization"
      }
    },
    "scripts": {
      "build": "xsd2php convert config.yml 'sepxml/srvRefund0.xsd'"
    }
}
